{
  "version": 2,
  "rewrites": [
    {
      "source": "/education/testbank",
      "destination": "/education/testbank/index.html"
    },
    {
      "source": "/education/testbank/:path((?!static).*)",
      "destination": "/education/testbank/index.html"
    }
  ],
  "headers": [
    {
      "source": "/education/testbank/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).svg",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/svg+xml"
        }
      ]
    },
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/(.*).css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    }
  ]
}
